        <script><![CDATA[function onLoad() {
	var urgency = parseInt(g_form.getValue('urgency'));
	if (isNaN(urgency)) {
		urgency = 3;
		g_form.setValue('urgency', urgency);
	}
	var impact = parseInt(g_form.getValue('impact'));
	if (isNaN(impact)) {
		impact = 3;
		g_form.setValue('impact', impact);
	}
	g_form.setValue('priority', impact + urgency - 1);
}]]></script>
